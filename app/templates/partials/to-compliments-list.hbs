<ul class="no-bullet connections-list">
  {{#each toAccept as |compliment|}}
  <div class="box rounded question-box">
    <div class="clearfix">
    <li>
      {{link-to compliment.from.name "profile" compliment.from.id class="user"}}
      has complimented to
      {{link-to 'You' "profile" 'me' class="user"}} 
       <h2>{{compliment.title}}</h2>
       <p>{{compliment.content}}</p>
    </li>
  </div>
  </div>
  {{/each}}
</ul>
